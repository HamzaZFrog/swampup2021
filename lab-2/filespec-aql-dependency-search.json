{
    "files": [
      {
        "aql": {
            "items.find": {
                "$and": [
                    {
                        "$or": [
                            {
                                "$and": [
                                    {
                                        "dependency.module.build.name": {
                                            "$eq": "docker-su-115"
                                        }
                                    },
                                    {
                                        "dependency.module.artifact.item.name": {
                                            "$eq": "manifest.json"
                                        }
                                    },
                                    {
                                        "dependency.module.build.number": {
                                            "$eq": "${build-number}"
                                        }
                                    }
                                ]
                            }
                        ]
                    }
                ]
        
            }
        },
        "sortBy": ["created"]
      }
    ]
  }
