
# current location
export SCRIPT_DIR=$(pwd)

# admin user
export ADMIN_USER=

# admin password
export ADMIN_PASSWORD=

# Service admin token
export token=

# JFrog Domains (JPD + Edges)
# Exemple : John Doe = Doej
export JFROG_PLATFORM=Doej.jfrog.io
export JFROG_EDGE=Doejedge.jfrog.io

export JFROG_PLATFORM_HTTP_PROTOCOL=https

# App name
export APP_ID=myApp

# Increment it everytime you run your lab
export APP_VERSION=1.1.40

# Increment it everytime you run your lab
export BUILD_NUMBER=1
